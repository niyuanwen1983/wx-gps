.wrap {
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
  flex-direction: column;
}

.content {
  flex: 1;
  overflow-y: scroll;
  /*width:100%;*/
}

.header {
  width: 100%;
  height: 88rpx;
  background: #465ffd;
  font-size: 28rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
}

.content .top {
  width: 100%;
  height: 280rpx;
  background: url(data:image/png;base64,) no-repeat top center;
  background-size: 100% 100%;
  padding: 60rpx 0 0 0;
}

.content .top .income {
  text-align: center;
  font-size: 26rpx;
  font-weight: 400;
  color: #fff;
  position: relative;
}

.content .top .income .money {
  display: block;
  font-weight: bold;
  font-size: 40rpx;
}

.content .top .income .white_arrow {
  width: 20rpx;
  height: 36rpx;
  background: url(data:image/png;base64,) no-repeat top center;
  background-size: 100% 100%;
  position: absolute;
  top: 36rpx;
  right: 240rpx;
}

.content .middle {
  width: 664rpx;
  height: 180rpx;
  background: url(data:image/png;base64,) no-repeat top center;
  background-size: 100% 100%;
  margin: -120rpx auto 0 auto;
  padding: 40rpx 24rpx 0 24rpx;
}

.content .middle view {
  width: 50%;
  float: left;
  text-align: center;
  color: #465ffd;
}

.content .middle view .num {
  font-size: 32rpx;
  font-weight: bold;
  display: block;
}

.content .middle view .wrd {
  font-size: 32rpx;
  font-weight: 400;
}

.content .middle view.left {
  position: relative;
}

.content .middle view.left:after {
  content: "";
  position: absolute;
  top: 20rpx;
  right: 0;
  width: 1px;
  height: 56rpx;
  background-color: #445dfd;
}

.content .bottom {
  background-color: #fff;
}

.content .bottom view {
  border-bottom: 1px solid #eee;
  padding-left: 40rpx;
  font-size: 0;
  height: 112rpx;
  width: 708rpx;
}

.content .bottom view i {
  float: left;
  width: 48rpx;
  height: 48rpx;
  margin: 36rpx 36rpx 0 0;
}

.content .bottom view i.icon1 {
  background: url(data:image/png;base64,) no-repeat top center;
  background-size: 100% 100%;
}

.content .bottom view i.icon2 {
  background: url(data:image/png;base64,) no-repeat top center;
  background-size: 100% 100%;
}

.content .bottom view i.icon3 {
  background: url(data:image/png;base64,) no-repeat top center;
  background-size: 100% 100%;
}

.content .bottom view p {
  width: 592rpx;
  font-size: 32rpx;
  padding: 36rpx 24rpx 0 0;
  float: left;
}

.content .bottom view p image {
  float: right;
  width: 20rpx;
  height: 36rpx;
  margin: 4rpx 0 0 0;
}

.arrow {
  height: 24rpx;
  width: 24rpx;
}
