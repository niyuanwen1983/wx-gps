page {
  height: 100%;
}

.wrap {
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
  flex-direction: column;
}

.content {
  flex: 1;
  overflow-y: scroll;
}

.header {
  width: 100%;
  height: 88rpx;
  background: #465ffd;
  font-size: 28rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
}

/*安装位置*/

.site {
  padding: 0.53rem 0.40rem;
}

.site img {
  display: block;
  width: 9.20rem;
  height: 7.33rem;
  margin: 0 auto;
}

.site .hide {
  display: none;
}

.site ul {
  margin: 0.87rem 0 0.73rem 0;
}

.site ul li {
  width: 50%;
  float: left;
  font-size: 28rpx;
  color: #666;
  margin-bottom: 0.40rem;
}

.site ul li label {
  display: inline-block;
  position: relative;
}

.site ul li label span {
  width: 15px;
  height: 15px;
  display: inline-block;
  position: absolute;
  top: 3px;
  left: -7px;
}

.clearfix{
  margin:20rpx 0 0 0;
}

li>image {
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: contain;
  height: 32rpx;
  margin:0 20rpx 0 0;
  width: 32rpx;
}

.item-checked {
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: contain;
  height: 32rpx;
  margin:0 20rpx 0 0;
  width: 32rpx;
}

.site .finishBtn {
  display: block;
  width: 598rpx;
  height: 76rpx;
  background: rgba(68, 93, 253, 1);
  border-radius: 38px;
  outline: none;
  border: 0;
  margin: 56rpx auto 36rpx auto;
  font-size: 30rpx;
  color: #fefefe;
  padding:22rpx 0 0 0;
}

.content>image {
  height: 560rpx;
  margin:0 0 20rpx 0;
  width: 720rpx;
}

.everblue {
  background-color: #00f !important;
}