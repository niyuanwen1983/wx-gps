.wrap {
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
}

.content {
  flex: 1;
  overflow-y: scroll;
  width: 100%;
  background: #fff;
}

.header {
  width: 100%;
  height: 88rpx;
  background: #465ffd;
  font-size: 28rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
}

.content .logo {
  display: block;
  width: 256rpx;
  height: 256rpx;
  margin: 80rpx auto 108rpx auto;
}

.inputBox {
  height: 88rpx;
  border: 1rpx solid #e8e7e7;
  border-radius: 44rpx;
  margin: 24rpx auto 0 auto;
  width: 600rpx;
}

.inputBox i {
  display: block;
  width: 24rpx;
  height: 36rpx;
  float: left;
  margin: 28rpx 16rpx 0 36rpx;
}

.inputBox i.icon1 {
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100%;
}

.inputBox i.icon2 {
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100%;
}

.inputBox input {
  border: 0;
  outline: none;
  width: 280rpx;
  height: 28rpx;
  float: left;
  font-size: 28rpx;
  color: #333;
  margin: 20rpx 0 0 0;
}

::-webkit-input-placeholder {
  color: #ccc;
}

:-moz-placeholder {
  color: #ccc;
}

::-moz-placeholder {
  color: #ccc;
}

:-ms-input-placeholder {
  color: #ccc;
}

.form .inputBox .getcode {
  float: right;
  font-size: 28rpx;
  color: #465ffd;
  background-color: transparent;
  appearance: none;
  -moz-appearance: none;
  -webkit-appearance: none;
  margin: 32rpx 40rpx 0 0;
  width: 180rpx;
}

.getcode:after{
  border:none;
}

.loginBtn {
  display: block;
  width: 600rpx;
  height: 76rpx;
  background-color: #f0f0f0;
  margin: 208rpx auto 0 auto;
  outline: none;
  appearance: none;
  -moz-appearance: none;
  -webkit-appearance: none;
  border-radius: 44rpx;
  font-size: 28rpx;
  color: #999;
  padding:24rpx 0 0 0;
  text-align: center;
}

.loginBtn::after {
  border: none;
}